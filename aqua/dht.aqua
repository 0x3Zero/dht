module Dht declares *

data FdbDht:
  public_key: string
  cid: string
  key: string

data FdbResult:
  success: bool
  err_msg: string

service Dht("Dht"):
  get_latest_record_by_pk_and_key(key: string, public_key: string) -> FdbDht
  get_records_by_key(key: string) -> []FdbDht
  initialize() -> FdbResult
  insert(key: string, cid: string, public_key: string, signature: string, message: string) -> FdbResult
  shutdown() -> FdbResult
